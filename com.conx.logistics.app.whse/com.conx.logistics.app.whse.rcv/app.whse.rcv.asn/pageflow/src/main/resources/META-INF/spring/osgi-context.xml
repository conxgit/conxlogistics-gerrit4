	<osgi:service ref="addAsnDropOffInfoPage"  interface="com.conx.logistics.kernel.pageflow.services.IModelDrivenPageFlowPage">
		<osgi:service-properties>
			<entry key="PROCESS_ID" value="whse.rcv.asn.CreateNewASNByOrgV1.0"/>
			<entry key="TASK_NAME" value="AddASNDropOffInfo"/>
		  </osgi:service-properties>		
	</osgi:service>
	<osgi:service ref="confirmAsnPage"  interface="com.conx.logistics.kernel.pageflow.services.IModelDrivenPageFlowPage">
		<osgi:service-properties>
			<entry key="PROCESS_ID" value="whse.rcv.asn.CreateNewASNByOrgV1.0"/>
			<entry key="TASK_NAME" value="ConfirmASN"/>
		  </osgi:service-properties>		
